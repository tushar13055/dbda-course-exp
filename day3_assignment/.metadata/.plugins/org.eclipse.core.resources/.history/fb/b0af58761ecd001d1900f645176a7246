package Array;

public class Box {
	private int length, width, height;
	
	Box(int length, int width, int height) {
		this.length = length;
		this.width = width;
		this.height = height;
	}
	
	public String getBoxDimentions() {
		return ("Length: " + length + " Width: " + width + " Height: " + height);
	}
}
